{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["/src/modules/streamedby.js", "/src/modules/playerutils.js", "/src/modules/autoplay.js", "readme.md"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
          "outputSourceFiles": true
        }
    },
    "opts": {
      "destination": "./documentation/",
      "recurse": true
    }
}
